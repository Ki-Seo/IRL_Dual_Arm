<!--		 base 		-->
<link name="base">
    <collision>
    <origin rpy="0 0 0" xyz="0 0 0.05"/>
      	<geometry>
        <mesh filename="package://irl_dual_arm/urdf/mesh/base.dae" scale="1 1 1"/>
