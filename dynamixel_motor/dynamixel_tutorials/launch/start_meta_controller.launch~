    <rosparam file="$(find dynamixel_tutorials)/tilt.yaml" command="load"/>
    <node name="controller_spawner" pkg="dynamixel_controllers" type="controller_spawner.py"
          args="--manager=dxl_manager
                --port dxl_USB0
                joint3_controller                       
                joint4_controller
                joint5_controller
                "
          output="screen"/>
          
